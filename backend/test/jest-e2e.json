{
  "moduleFileExtensions": ["js", "json", "ts", "tsx"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1"
  },
  "globalSetup": "<rootDir>/global-setup.ts"
}
