  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^prisma/(.*)$": "<rootDir>/prisma/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "src/**/*.(t|j)sx"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
